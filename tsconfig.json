{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react-native",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "moduleResolution": "bundler",
    "customConditions": [
      "react-native"
    ],
    "types": [
      "react",
      "react-native",
      "expo-router"
    ]
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ],
  "paths": {
  "@fitbit/*": ["screens/Fitbit/*"]
}
}
